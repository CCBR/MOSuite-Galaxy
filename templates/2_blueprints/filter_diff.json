      "defaultValue": "any",
      "condition": null,
      "content": null,
      "objectPropertyReference": null
    },
    {
      "key": "include_estimates",
      "displayName": "Include Estimates",
      "description": "Column names of estimates to include. Default: `c(\"FC\", \"logFC\", \"tstat\", \"pval\", \"adjpval\")`",
      "paramType": "MULTISELECT",
      "paramGroup": "Advanced",
